{
  "name": "hugo-bootstrap",
  "version": "1.0.0",
  "description": "Starter application for building Hugo site with bootstrap plugins and components",
  "main": "index.html",
  "scripts": {
    "start:hugo": "hugo server -D --disableFastRender",
    "install": "gulp install",
    "cleanLibs": "gulp cleanLibs",
    "cleanRoot": "gulp cleanRoot",
    "clean": "gulp clean",
    "start": "npm-run-all install start:hugo",
    "build": "npm-run-all clean install hugo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FpRezDev/hugo-bootstrap.git"
  },
  "keywords": [
    "hugo",
    "bootstrap"
  ],
  "author": "FpRezDev <fprez.developer@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FpRezDev/hugo-bootstrap/issues"
  },
  "homepage": "https://github.com/FpRezDev/hugo-bootstrap#readme",
  "devDependencies": {
    "bootstrap": "^4.5.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "jquery": "^3.5.1",
    "merge-stream": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1"
  }
}
